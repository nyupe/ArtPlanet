<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
	public String memberNo;
	public String categorieNo;
	public String blogNo;
 -->
<mapper namespace="mybatis.CategorieRelation">

	<insert id="insertCategorieRelation" parameterType="java.util.Map">
		INSERT INTO categorieRelation(memberNo, categorieNo, blogNo)
			VALUES(#{memberNo}, #{categorieNo}, #{blogNo})
	</insert>

</mapper>